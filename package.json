    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "moment": "^2.18.1",
    "mongoose": "^5.0.15",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "pdfmake": "^0.1.31",
    "qn": "^1.3.0",
