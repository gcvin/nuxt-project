  "devDependencies": {
    "@babel/register": "^7.0.0-beta.44",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-standard": "^3.0.0-beta.6",
    "axios": "^0.18.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "bytes": "^3.0.0",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.9",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
